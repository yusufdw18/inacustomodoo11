<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
	    <record id="inherit_invoice_supplier_form_view" model="ir.ui.view">
	        <field name="name">inherit.invoice.supplier.form.view</field>
	        <field name="model">account.invoice</field>
	        <field name="inherit_id" ref="account.invoice_supplier_form"/>
	        <field name="arch" type="xml">
	            <xpath expr="//tree/field[@name='discount']" position="attributes">
					<attribute name="string">Discount</attribute>
				</xpath>
	        </field>
	    </record>
	</data>
</odoo>
